<header class="header">
    <nav class="text-center">
        <a href="/">Home</a>
        <a href="/pokemon">Favorites</a>
    </nav>
</header>

<div id="faves" class="text-center">
    <h1 class="title"> Here are your favorite pokemons! </h1>
    <div class="favePoke" class="text-center">
        <% pokemon.forEach(pokemon => { %>
            <h3>
                <a href="/pokemon/<%= pokemon.name %>">
                    <%= pokemon.name %>
                </a>
            </h3>
        <% }); %>
    </div>
</div>